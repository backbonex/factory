{
  "name": "backbone.factory",
  "version": "1.1.1",
  "description": "Factory pattern implementation for backbone views",
  "main": "lib/Factory",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "./node_modules/.bin/jscoverage lib && ./node_modules/.bin/mocha-browser tests/tests.html && rm -rf lib-cov",
    "cov": "./node_modules/.bin/jscoverage lib && ./node_modules/.bin/mocha-browser tests/tests.html -R html-cov > /tmp/report.html && gnome-open /tmp/report.html",
    "coveralls": "./node_modules/.bin/jscoverage lib && mocha-browser tests/tests.html -R lcov | ./node_modules/.bin/coveralls && rm -rf lib-cov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/backbonex/factory.git"
  },
  "keywords": [
    "backbone",
    "factory",
    "views",
    "selectors",
    "initialization"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/backbonex/factory/issues"
  },
  "devDependencies": {
    "jscoverage": "~0.5.9",
    "coveralls": "~2.11.2",
    "mocha-browser": "~0.2.0"
  }
}
